{
  "name": "Hygro-Rechner",
  "description": "Berechnet Taupunkt und absolute Luftfeuchtigkeit aus Temperatur und Luftfeuchtigkeit",
  "input": {
    "temperature": {
      "name": "Temperatur",
      "description": "Temperatureingabe"
    },
    "humidity": {
      "name": "Luftfeuchtigkeit",
      "description": "Relative Luftfeuchtigkeitseingabe"
    }
  },
  "output": {
    "dewPoint": {
      "name": "Taupunkt",
      "description": "Berechnete Taupunkttemperatur"
    },
    "absoluteHumidity": {
      "name": "Absolute Feuchtigkeit",
      "description": "Berechnete absolute Luftfeuchtigkeit"
    }
  }
}
